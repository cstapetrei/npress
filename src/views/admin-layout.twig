{% set currentUrl = router.getRewriteUri() %}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <link rel="stylesheet" href="/css/all.min.css">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/noty.css">
    <link rel="stylesheet" href="/css/admin-npress.css">
    <link rel="stylesheet" href="/css/loader.css">
    <link rel="stylesheet" href="/css/flatpickr.min.css">
    <link rel="stylesheet" href="/vendor/adminlte/css/adminlte.min.css">
    <script type="text/javascript" src="/vendor/fetch.umd.js"></script>
    <script type="text/javascript" src="/vendor/twig.js"></script>

    <!-- for AdminLte !-->
        <script type="text/javascript" src="/vendor/jquery-3.5.1.min.js"></script>
        <script type="text/javascript" src="/vendor/adminlte/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script type="text/javascript" src="/vendor/adminlte/js/adminlte.min.js"></script>
    <!-- for AdminLte !-->
    {% block header_css %}
    {% endblock %}

    {% block header_js %}
    {% endblock %}
</head>
<body class="hold-transition sidebar-mini {{ body_class|default('') }}">
<div class="wrapper">

{% block navigation %}
    {% include 'admin/partials/nav.twig' with {
      settings: settingsMap,
      show_search: show_search,
      nav_title: nav_title
    } only %}
{% endblock %}

{% block sidebar %}
    {% include 'admin/partials/sidebar.twig' with { settings: settingsMap, menu: menu } only %}
{% endblock %}

<!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        {% block header %}
        {% endblock %}
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <div class="content">
      <div class="container-fluid">
        {% block body %}
        {% endblock %}
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content -->
  </div>
<!-- /.content-wrapper -->

<!-- Control Sidebar -->
<aside class="control-sidebar control-sidebar-dark">
    {% block control_sidebar_content %}
    {% endblock %}
</aside>
<!-- /.control-sidebar -->

{% block footer %}
    {% include 'admin/partials/footer.twig' with { settings: settingsMap, menu: menu } only %}
{% endblock %}

</div>

{% block templates %}{% endblock %}

<script type="text/javascript" src="/vendor/bootstrap-native.min.js"></script>
<script type="text/javascript" src="/vendor/flatpickr.js"></script>
<script type="text/javascript" src="/vendor/noty.min.js"></script>
<script type="text/javascript" src="/vendor/npress.js"></script>
<script type="text/javascript" src="/vendor/npress_admin_api.js"></script>
<script type="text/javascript" src="/vendor/npress-modal.js"></script>
<script type="text/javascript" src="/vendor/npress-pagination.js"></script>
<script type="text/javascript" src="/vendor/npress-media-modal.js"></script>
<script type="text/javascript" src="/vendor/npress-search-handler.js"></script>
<script type="text/javascript" src="/vendor/npress-history-handler.js"></script>

{% block footer_js %}{% endblock %}
</body>
</html>
