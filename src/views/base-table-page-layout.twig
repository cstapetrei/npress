{% extends "admin-layout.twig" %}

{% block footer_js %}
    <script type="text/javascript" src="/js/admin/base-admin-page.js"></script>
{% endblock %}


{% block body %}
    <div class="card">
        <div class="card-header page-card-header d-flex align-items-center">
            <button class="btn btn-primary js-new-item mx-1">New item</button>
            <button class="btn btn-danger js-bulk-delete mx-1">Delete rows</button>
            <div class="form-check form-check-inline mx-1">
                <input class="form-check-input js-show-checkboxes-input" type="checkbox" id="show-checkboxes-input">
                <label class="form-check-label" for="show-checkboxes-input">Show checkboxes</label>
            </div>
        </div>
        <div class="card-body js-data-table-wrapper"></div>
        <div class="card-footer js-pagination-wrapper"></div>
    </div>
{% endblock %}