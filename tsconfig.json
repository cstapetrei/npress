{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "sourceMap": true,
      "outDir": "./build/src",
      "strict": true,
      "noImplicitAny": true,
      "strictPropertyInitialization": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": [
        "src/**/*",
        "env.ts",
        "server.ts"
    ],
    "exclude": [
        "src/static",
    ]
}
